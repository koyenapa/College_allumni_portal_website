e.SET_DETAILS,\n        details: data,\n      });\n    });\n  };\n\n\n\n\n\n  return (\n    <>\n      <div className=\"flex flex-wrap flex-col items-center justify-center  py-8 bg-gradient-to-t from-emerald-300 to-indigo-300 pt-24 h-auto w-auto \">\n        <div>\n          <h1 className=\" font-serif text-3xl underline text-blue-900 bg-transparent\">\n            Register\n          </h1>\n        </div>\n\n        \n\n\n        <div className=\" flex justify-center items-center flex-wrap bg-transparent rounded-lg  mb-10 p-4 pl-14 md:ml-20 \">\n          <form className=\"\" action=\"post\">\n            <div>\n              <label\n                className=\"block mb-2 text-sm font-medium text-gray-900\"\n              >\n                Name\n              </label>\n              <label className=\"relative text-gray-600 justify-center w-64 lg:w-full items-center bg-white p-1 flex rounded-lg\">\n                <PersonIcon className=\"pl-1\" />\n                <input\n                  type=\"text\"\n                  value={name}\n              onChange={(e) => setName(e.target.value)}\n                  className=\" text-gray-900 sm:text-sm rounded-lg block w-60 lg:w-full p-1  focus:outline-none\"\n                  placeholder=\"Enter your Name...\"\n                  required\n                />\n              </label>\n            </div>\n            <div className=\" pb-2\">\n              <label\n                className=\"block mb-2 text-sm font-medium text-gray-900\"\n              >\n                Email\n              </label>\n              <label className=\"relative text-gray-600 justify-center w-64 lg:w-full items-center bg-white p-1 flex rounded-lg\">\n                <EmailIcon className=\"pl-1\" />\n                <input\n                  type=\"email\"\n                  \n                  className=\" text-gray-900 sm:text-sm rounded-lg block w-60 lg:w-full p-1  focus:outline-none\"\n                  placeholder=\"Enter your Email...\"