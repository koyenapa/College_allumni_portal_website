ta = async () => {\n    await getAllDetails().then((data) => {\n      dispatch({\n        type: actionType.SET_DETAILS,\n        details: data,\n      });\n    });\n  };\n\n  return (\n    <div className=\"w-full h-full align-middlen justify-center bg-gradient-to-t from-emerald-300 to-indigo-300 text-white\">\n      <div className=\"w-full min-h-screen flex flex-col items-center justify-center\">\n        <div>\n          <p className=\" text-4xl font-serif text-blue-900 underline pb-5\">Registration</p>\n        </div>\n        <div className=\"w-[90%] md:w-[50%] border border-gray-300 rounded-lg p-4 flex flex-col items-center justify-center gap-4\">\n          {fields && (\n            <motion.p\n              initial={{ opacity: 0 }}\n              animate={{ opacity: 1 }}\n              exit={{ opacity: 0 }}\n              className={`w-full p-2 rounded-lg text-center text-lg font-semibold ${\n                alertStatus === \"danger\"\n                  ? \"bg-red-400 text-red-800\"\n                  : \"bg-emerald-400 text-emerald-800\"\n              }`}\n            >\n              {msg}\n            </motion.p>\n          )}\n\n          <div className=\"w-full py-2 border-b border-gray-300 flex items-center gap-2 text-white\">\n            <input\n              type=\"text\"\n              required\n              placeholder=\"Name\"\n              value={name}\n              onChange={(e) => setName(e.target.value)}\n              className=\"w-full h-full text-lg bg-transparent outline-none border-none placeholder:text-black text-textColor\"\n            />\n          </div>\n\n          <div className=\"w-full py-2 border-b border-gray-300 flex items-center gap-2\">\n            <input\n              type=\"text\"\n              required\n              placeholder=\"pass-out year\"\n              value={passout_year}\n              onChange={(e) => setPassout_year(e.target.value)}\n              className=\"w-full h-full text-lg bg-transparent outline